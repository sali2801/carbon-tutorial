{
  "name": "carbon-tutorial",
  "version": "0.0.0",
  "private": true,
  "repository": "git@github.com:carbon-design-system/carbon-tutorial.git",
  "bugs": "https://github.com/carbon-design-system/carbon-tutorial/issues",
  "license": "Apache-2.0",
  "scripts": {
    "build": "react-scripts build",
    "ci-check": "yarn format:diff",
    "clean": "yarn cache clean && yarn install",
    "eject": "react-scripts eject",
    "format": "prettier --write \"**/*.{js,md,scss}\"",
    "format:diff": "prettier --list-different \"**/*.{js,md,scss}\"",
    "format:staged": "prettier --write",
    "start": "react-scripts start",
    "test": "react-scripts test"
  },
  "dependencies": {
    "@carbon/charts": "^0.41.67",
    "@carbon/charts-react": "^0.41.67",
    "@carbon/colors": "^10.26.0",
    "@carbon/grid": "10.17.0",
    "@carbon/icons-react": "^10.33.0",
    "@carbon/themes": "^10.35.0",
    "@types/carbon-components-react": "^7.34.0",
    "@types/carbon__icons-react": "^10.31.0",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.0",
    "carbon-components": "10.25.0",
    "carbon-components-react": "7.25.0",
    "carbon-icons": "7.0.7",
    "d3": "5.x",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.5",
    "react": "16.10.0",
    "react-dom": "16.10.0",
    "react-redux": "^7.2.4",
    "react-router-dom": "5.0.0",
    "react-scripts": "4.0.1",
    "redux": "^4.1.0",
    "reselect": "^4.0.0",
    "typescript": "^3.9.3"
  },
  "devDependencies": {
    "@commitlint/cli": "7.5.2",
    "@commitlint/config-conventional": "7.5.0",
    "@types/react-redux": "^7.1.12",
    "@types/react-router-dom": "^5.1.6",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^3.0.0",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "prettier": "2.3.1",
    "sass": "1.29.0",
    "wait-for-expect": "3.0.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.js": [
      "yarn format:staged",
      "git add"
    ],
    "*.{scss,css}": [
      "yarn format:staged",
      "git add"
    ],
    "*.md": [
      "yarn format:staged",
      "git add"
    ]
  },
  "prettier": {
    "jsxBracketSameLine": true,
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
